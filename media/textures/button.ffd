sprite "button_on" {
    texture "button"
    clip 0 0 16 16
}
sprite "button_off" {
    texture "button"
    clip 16 0 16 16
}
texture "button" {
    file "button.png"
    isSmooth false
}
prop "button" {
    render "inactif" {
        type "sprite"
        rid "button_off"
        anchor 0.5 0.5
        pivot 0.5 0.5
        offset 0 0
        isRotating false
        angleOffset 0
        blend "alpha"
        isDefault true
        isBehind []
    }
    render "actif" {
        type "sprite"
        rid "button_on"
        anchor 0.5 0.5
        pivot 0.5 0.5
        offset 0 0
        isRotating false
        angleOffset 0
        blend "alpha"
        isDefault false
        isBehind []
    }
    hurtbox
    zOrderOffset -1
    material 0
}
prop "checkpoint" {
    render "inactif" {
        type "sprite"
        rid "button_off"
        anchor 0.5 0.5
        pivot 0.5 0.5
        offset 0 0
        isRotating false
        angleOffset 0
        blend "alpha"
        isDefault true
        isBehind []
    }
    render "actif" {
        type "sprite"
        rid "button_on"
        anchor 0.5 0.5
        pivot 0.5 0.5
        offset 0 0
        isRotating false
        angleOffset 0
        blend "alpha"
        isDefault false
        isBehind []
    }
    hurtbox
    controller "checkpoint"
    zOrderOffset -1
    material 0
}
